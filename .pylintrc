[MASTER]
disable=
    C0111, # missing-docstring
    C0103, # invalid-name
    C0301, # line-too-long
    W0311, # bad-indentation
    W0603, # global-statement
    R0903, # too-few-public-methods
    R0913, # too-many-arguments
    R0914, # too-many-locals

[FORMAT]
max-line-length=120

[REPORTS]
output-format=text
reports=no
score=no
